            <InnerShadow>
               <color>
                  <Color red="0.04145650565624237" green="0.01663011685013771" blue="0.5131579041481018" />
               </color>
            </InnerShadow>
         </effect>
      </Button>
      <Text fx:id="activeOrdersText" fill="WHITE" layoutX="24.0" layoutY="50.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Here you can see your Active Orders :">
