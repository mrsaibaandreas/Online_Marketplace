<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="306.0" prefWidth="557.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.supplierPageController">
   <children>
      <ImageView fx:id="backgroundImage" fitHeight="312.0" fitWidth="556.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@welcomepage-IMAGE.jpg" />
         </image>
      </ImageView>
      <ImageView fx:id="iamgeCenter" fitHeight="150.0" fitWidth="200.0" layoutX="198.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@OM-Text.png" />
         </image>
      </ImageView>
      <ImageView fx:id="iamgeCorner" fitHeight="108.0" fitWidth="115.0" layoutX="435.0" layoutY="203.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@OM-Image.png" />
         </image>
      </ImageView>
      <Button fx:id="loginButton" layoutX="227.0" layoutY="140.0" mnemonicParsing="false" onAction="#loginButtonAction" prefHeight="0.0" prefWidth="93.0" text="Login">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="goBackButton" layoutX="186.0" layoutY="272.0" mnemonicParsing="false" onAction="#goBackButton" text="Go back to WelcomePage !">
         <font>
            <Font size="13.0" />
         </font></Button>
      <Button fx:id="registerButtonSupplier" layoutX="227.0" layoutY="188.0" mnemonicParsing="false" onAction="#registerButtonSupplierAction" prefHeight="0.0" prefWidth="93.0" text="Register">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
