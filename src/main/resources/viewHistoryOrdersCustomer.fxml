<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="570.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.viewHistoryOrdersCustomer">
   <children>
      <ImageView fx:id="backgroundHistoryOfOrders" fitHeight="400.0" fitWidth="586.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@historyofOrdersBackground" />
         </image>
      </ImageView>
      <Text fx:id="textHistoryOfOrders" fill="WHITE" layoutX="14.0" layoutY="48.0" strokeType="OUTSIDE" strokeWidth="0.0" text="This is your History of Orders :">
         <font>
            <Font size="25.0" />
         </font>
      </Text>
      <Button fx:id="goBackButton" layoutX="23.0" layoutY="361.0" mnemonicParsing="false" onAction="#goBackButtonAction" text="Go back !" />
   </children>
</AnchorPane>
